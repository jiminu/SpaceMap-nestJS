        {{#if locals.isDesktop}}
        $.scrollIt({
            upKey: 38,
            downKey: 40,
            easing: 'easeInOutExpo',
            scrollTime: 1000,
            activeClass: 'active',
            onPageChange: null,
            topOffset: -100,
        })
        {{/if}}
    $('.section-thumb').each(function () {
        var section_thumb = $(this)
        var mySwiper = new Swiper($(section_thumb.find('.swiper-container')), {
            slidesPerView: 1,
            spaceBetween: 20,
            centeredSlides: true,
            {{#if locals.isMobile}}
                autoHeight: false,
            {{/if}}
            onSlideChangeStart: function () {
                var currentSlide = mySwiper.realIndex
                section_thumb.parent().find('.title').hide()
                section_thumb.parent().find('.title').eq(currentSlide).show()
            },
            onSlideChangeEnd: function () {
                if ($('.swiper-slide-prev video, .swiper-slide-next video').length) {
                    $('.swiper-slide-prev video, .swiper-slide-next video').get(0).pause()
                }
            },
            prevButton: $(section_thumb.find('.swiper-prev')),
            nextButton: $(section_thumb.find('.swiper-next')),
            pagination: $(section_thumb.find('.swiper-pagination')),
            paginationClickable: true,
        })
    })

