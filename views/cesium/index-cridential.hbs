<script type="text/javascript">
    $(function() {
        const token = $('#token').val();
        const headers = {'CSRF-Token': token};

        // $('.link-api').click(function(event) {
        //     event.preventDefault();
        //     checkAuth($(this).attr('href'), '#pop-login', headers);
        // });

        $('.ul-exp').on('mouseover', 'li', function() {
            $(this).removeClass('disabled');
        }).on('mouseout', 'li', function() {
            $(this).addClass('disabled');
        });
    });

    $('.slide-img-con').each(function() {
        var section_thumb = $(this);
        var mySwiper = new Swiper($(section_thumb.find('.swiper-container')), {
            slidesPerView: 1,
            spaceBetween: 20,
            centeredSlides: true,
            autoHeight: true,
            onSlideChangeStart: function() {},
            onSlideChangeEnd: function() {
                if ($('.swiper-slide-prev video, .swiper-slide-next video').length) {
                    $('.swiper-slide-prev video, .swiper-slide-next video').get(0).pause();
                }
            },
            prevButton: $(section_thumb.find('.swiper-prev')),
            nextButton: $(section_thumb.find('.swiper-next')),
            pagination: $(section_thumb.find('.swiper-pagination')),
            paginationClickable: true,
        });
    });
</script>